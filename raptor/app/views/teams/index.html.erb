<h1 class="text-center"> Teams</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Twitter</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= link_to team.display_name, team %></td>
        <td><%= link_to team.twitter_url, team.twitter_url %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

